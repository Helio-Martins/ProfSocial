init python:
    def dragging(drags, drop):
        if not drop:
            return
        if drop.drag_name == "Frases"+drags[0].drag_name:
            drags[0].snap(0, 0)
            if store.options_picked[int(drags[0].drag_name)]:
                store.drag_num += 1
                store.options_picked[int(drags[0].drag_name)] = False
            #if situation % 10 != 9:
            #    store.concordos[int(drags[0].drag_name)] = 0
            store.concordos[int(drags[0].drag_name)] = 0
        elif drop.drag_name == "Concordo t"+drags[0].drag_name:
            drags[0].snap(328*4, 0)
            if not store.options_picked[int(drags[0].drag_name)]:
                store.drag_num -= 1
                store.options_picked[int(drags[0].drag_name)] = True
            #if situation % 10 != 9:
            #    store.concordos[int(drags[0].drag_name)] = 2:
            store.concordos[int(drags[0].drag_name)] = 4
        elif drop.drag_name == "Concordo p"+drags[0].drag_name:
            drags[0].snap(328*3, 0)
            if not store.options_picked[int(drags[0].drag_name)]:
                store.drag_num -= 1
                store.options_picked[int(drags[0].drag_name)] = True
            #if situation % 10 != 9:
            #    store.concordos[int(drags[0].drag_name)] = 1
            store.concordos[int(drags[0].drag_name)] = 3
        elif drop.drag_name == "Discordo p"+drags[0].drag_name:
            drags[0].snap(328*2, 0)
            if not store.options_picked[int(drags[0].drag_name)]:
                store.drag_num -= 1
                store.options_picked[int(drags[0].drag_name)] = True
            #if situation % 10 != 9:
            #    store.concordos[int(drags[0].drag_name)] = -1
            store.concordos[int(drags[0].drag_name)] = 2
        else:
            drags[0].snap(328*1, 0)
            if not store.options_picked[int(drags[0].drag_name)]:
                store.drag_num -= 1
                store.options_picked[int(drags[0].drag_name)] = True
            #if situation % 10 != 9:
            #    store.concordos[int(drags[0].drag_name)] = -2
            store.concordos[int(drags[0].drag_name)] = 1

        renpy.restart_interaction()
        return

    def ordering(drags, drop):
        if not drop:
            return True

        for iter_i in range(0, len(store.drag_opts)):
            if drop.drag_name == str(iter_i)+".":
                #drags[0].snap(220, 250+75*i)
                temp_opt = drag_options_curr[iter_i]
                store.drag_options_curr[iter_i] = drag_options_curr[int(drags[0].drag_name)]
                store.drag_options_curr[int(drags[0].drag_name)] = temp_opt

        renpy.restart_interaction()
        return True

